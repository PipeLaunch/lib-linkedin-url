{
  "name": "lib-linkedin-url",
  "version": "1.0.0-beta.1",
  "description": "Utility library to work with LinkedIn profiles URLs: get the canonical url, validate, etc.",
  "keywords": [
    "linkedin",
    "canonical",
    "profile",
    "company"
  ],
  "license": "MIT",
  "author": "samuel@pipelaunch.com",
  "repository": "https://github.com/PipeLaunch/lib-linkedin-url.git",
  "bugs": "https://github.com/PipeLaunch/lib-linkedin-url/issues",
  "homepage": "https://github.com/PipeLaunch/lib-linkedin-url#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "!**/__tests__/**/*"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "run": "tsc && node ./dist/index.js",
    "clean": "rm -rf dist",
    "lint": "tsc --noEmit && eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test:unit": "jest",
    "test": "npm run lint && npm run test:unit",
    "prepublishOnly": "npm run test && npm run build",
    "publish": "npm publish --tag beta",
    "changelog": "auto-changelog -p && git add CHANGELOG.md"
  },
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "@types/node": "^16.11.65",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "auto-changelog": "^2.4.0",
    "eslint": "^8.25.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "git stash --keep-index --include-untracked && npm run build && echo 'building done!' && git add --all && git stash pop"
    }
  }
}
